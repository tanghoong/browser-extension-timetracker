{
  "name": "time-tracker-extension",
  "version": "1.0.0",
  "description": "Privacy-first browser extension to track time spent and visits on websites",
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint extension/**/*.js",
    "lint:fix": "eslint extension/**/*.js --fix",
    "format": "prettier --write \"extension/**/*.{js,html,css,json}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "node scripts/build.js",
    "build:chrome": "node scripts/build.js chrome",
    "build:firefox": "node scripts/build.js firefox",
    "build:edge": "node scripts/build.js edge",
    "package": "node scripts/package.js",
    "web-ext:run": "web-ext run --source-dir=extension",
    "web-ext:lint": "web-ext lint --source-dir=extension"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "jest": "^29.7.0",
    "web-ext": "^7.11.0"
  },
  "dependencies": {},
  "keywords": [
    "browser-extension",
    "time-tracker",
    "productivity",
    "privacy",
    "chrome-extension",
    "firefox-addon",
    "edge-extension"
  ]
}
